{
    "name": "tendoo/cms",
    "description": "Add cms feature to a laravel application.",
    "license": "MIT",
    "authors": [
        {
            "name": "blair Jersyer",
            "email": "carlosjohnsonluv2004@gmail.com"
        }
    ],
    "require": {
        "php" : ">=7.0.0",
        "barryvdh/laravel-debugbar": "^3.1",
        "jackiedo/dotenv-editor" : "^1.0",
        "nikic/php-parser": "^4.0",
        "orchestra/parser": "^3.5",
        "tormjens/eventy": "^0.5.3",
        "ixudra/curl": "6.*",
        "gumlet/php-image-resize": "1.8.*",
        "doctrine/dbal" : "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Tendoo\\": "src/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Tendoo\\ServiceProvider",
                "Tendoo\\Core\\Providers\\TendooAppServiceProvider",
                "Tendoo\\Core\\Providers\\TendooEventServiceProvider",
                "Tendoo\\Core\\Providers\\TendooModulesServiceProvider",
                "Tendoo\\Core\\Providers\\TendooRouteServiceProvider",
                "Tendoo\\Core\\Providers\\TendooUserOptionsServiceProvider"                
            ]
        }
    },
    "require-dev": {
        
    }
}
