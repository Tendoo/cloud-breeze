{
  "name": "tendoo-setup",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod --vendorChunk --outputHashing=none",
    "build-ct": "cd projects/cloud-tools; ng build; npm publish",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-libraries": "npm run publish-table; npm run publish-dialog",
    "publish-core": "ng build @cloud-breeze/core && (cd dist/cloud-breeze/core) && npm publish --access public",
    "publish-table": "ng build @cloud-breeze/crud-table && (cd dist/cloud-breeze/crud-table) && npm publish --access public",
    "publish-dialog": "ng build @cloud-breeze/dialog && (cd dist/cloud-breeze/dialog) && npm publish --access public",
    "remove-libraries": "npm remove @cloud-breeze/core ; npm remove @cloud-breeze/crud-table ; npm remove @cloud-breeze/dialog",
    "ilib": "npm i @cloud-breeze/core && npm i @cloud-breeze/crud-table && npm i @cloud-breeze/dialog"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.12",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/flex-layout": "^7.0.0-beta.24",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/material": "^7.3.7",
    "@angular/material-moment-adapter": "^7.3.7",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "core-js": "^2.6.5",
    "hammerjs": "^2.0.8",
    "install": "^0.12.2",
    "material-design-icons": "^3.0.1",
    "moment": "^2.24.0",
    "ngx-captcha": "^6.4.0",
    "ngx-cookie-service": "^2.1.0",
    "ngx-dropzone-wrapper": "^7.2.1",
    "placeholder-loading": "^0.1.5",
    "rxjs": "~6.3.3",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.8",
    "@angular-devkit/build-ng-packagr": "~0.10.0",
    "@angular/cli": "^7.3.8",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "protractor": "^5.4.2",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.1"
  }
}
