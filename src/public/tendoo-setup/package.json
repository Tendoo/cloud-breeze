{
  "name": "tendoo-setup",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod --vendorChunk --outputHashing=none",
    "build-ct": "cd projects/cloud-tools; ng build; npm publish",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-libraries": "npm run publish-table; npm run publish-dialog",
    "publish-core": "ng build @cloud-breeze/core && cd dist/cloud-breeze/core && npm publish --access public",
    "publish-table": "ng build @cloud-breeze/crud-table && (cd dist/cloud-breeze/crud-table) && npm publish --access public",
    "publish-dialog": "ng build @cloud-breeze/dialog && (cd dist/cloud-breeze/dialog) && npm publish --access public",
    "remove-libraries": "npm remove @cloud-breeze/core ; npm remove @cloud-breeze/crud-table ; npm remove @cloud-breeze/dialog",
    "ilib": "npm i @cloud-breeze/core && npm i @cloud-breeze/crud-table && npm i @cloud-breeze/dialog"
  },
  "resolutions": {
    "caniuse-lite": "1.0.30000971"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.0.0",
    "@angular/cdk": "^8.0.0",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/flex-layout": "^8.0.0-beta.26",
    "@angular/forms": "~8.0.0",
    "@angular/http": "~7.2.15",
    "@angular/material": "^8.0.0",
    "@angular/material-moment-adapter": "^8.0.0",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "core-js": "^3.1.3",
    "hammerjs": "^2.0.8",
    "install": "^0.12.2",
    "material-icons": "^0.3.1",
    "moment": "^2.24.0",
    "ngx-captcha": "^8.0.1",
    "ngx-cookie-service": "^3.0.3",
    "ngx-dropzone-wrapper": "^9.0.0",
    "ngx-image-cropper": "^3.1.4",
    "placeholder-loading": "^0.2.5",
    "rxjs": "~6.4.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.800.1",
    "@angular-devkit/build-ng-packagr": "^0.800.1",
    "@angular/cli": "^8.0.1",
    "@angular/compiler-cli": "^8.0.0",
    "@angular/language-service": "~8.0.0",
    "@types/jasmine": "^3.3.13",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~12.0.4",
    "codelyzer": "~5.1.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^5.2.0",
    "protractor": "^5.4.2",
    "ts-node": "~8.2.0",
    "tsickle": "0.35.0",
    "tslib": "^1.9.0",
    "tslint": "~5.17.0",
    "typescript": "~3.4.5"
  }
}
