      "root": "projects/brookr",
      "sourceRoot": "projects/brookr/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "../../../../../laravel-7001/modules/Brookr/Public/brookr",
            "index": "projects/brookr/src/index.html",
            "main": "projects/brookr/src/main.ts",
            "polyfills": "projects/brookr/src/polyfills.ts",
